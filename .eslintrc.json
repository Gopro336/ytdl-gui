{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "prettier/prettier": "error"
    }
}
